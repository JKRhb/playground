{
  "info": {
    "title": "Example Bluetooth mesh Generic Default Transition Time Model", 
    "version": "20200710", 
    "copyright": "Copyright Bluetooth SIG, Inc. All rights reserved.", 
    "license": "https://github.com/one-data-model/oneDM/blob/master/LICENSE"
  }, 
  "namespace": {
    "pg": "https://onedm.org/playground/"
  }, 
  "defaultNamespace": "pg", 
  "sdfObject": {
    "GenericDefaultTransitionTime": {
      "sdfProperty": {
        "DefaultTransitionTime": {
          "sdfRef": "#/sdfObject/GenericDefaultTransitionTime/sdfData/TransitionTimeData"
        }
      },
      "sdfAction": {
        "DefaultTransitionTimeGet" : {
          "description": "Get the default transition time",
          "sdfInputData": [
            "#/sdfObject/GenericLevel/sdfAction/LevelSet/sdfData/Level",
            "#/sdfObject/GenericLevel/sdfAction/LevelSet/sdfData/TransitionTime",
            "#/sdfObject/GenericLevel/sdfAction/LevelSet/sdfData/Delay"
          ],
          "sdfData": {
            "TransitionTimeSteps": {
              "sdfRef": "#/sdfObject/GenericLevel/sdfData/TransitionTimeSteps"
            },
            "StepResolution": {
              "sdfRef": "#/sdfObject/GenericLevel/sdfData/StepResolution"
            }
          }
        },
        "DefaultTransitionTimeSet" : {
          "description": "Set the default transition time",
          "sdfData": {
            "TransitionTimeSteps": {
              "sdfRef": "#/sdfObject/GenericLevel/sdfData/TransitionTimeSteps"
            },
            "StepResolution": {
              "sdfRef": "#/sdfObject/GenericLevel/sdfData/StepResolution"
            }
          }
        }
      },
      "sdfData": {
        "TransitionTimeSteps": {
          "description": "Number of steps in the transition",
          "type": "number",
          "minimum": 0,
          "maximum": 63,
          "multipleOf": 1
        },
        "StepResolution": {
          "description": "Step Resolution, time per step",
          "enum": [
            "100 Milliseconds", 
            "1 Second", 
            "10 Seconds", 
            "10 Minutes"
          ]
        }
      }
    }
  }
}